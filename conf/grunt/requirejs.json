{
    "compile":{
        "options":{
            "baseUrl": "src/",
            "paths": {
                "backbone": "../node_modules/backbone/backbone",
                "underscore": "../node_modules/underscore/underscore",
                "jquery": "../node_modules/jquery/dist/jquery"
            },
            "shim": {
                "backbone": {
                    "exports": "Backbone"
                },
                "underscore": {
                    "exports": "_"
                }
            },
            "optimize": "uglify2",
            "preserveLicenseComments": false,
            "name": "ribcage",
            "out": "build/ribcage.js"
        }
    }
}
